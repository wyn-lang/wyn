// Simplified Code Generator - Stage 3
// Reads tokens and generates basic C code

fn generate_c(tokens: string) -> string {
    var output = "#include <stdio.h>\n\nint add(int a, int b) {\n  return a + b;\n}\n\nint main() {\n  return add(5, 3);\n}\n"
    return output
}

fn main() -> int {
    var tokens = File::read("tokens.txt")
    var c_code = generate_c(tokens)
    File::write("output.c", c_code)
    print("Codegen: ")
    print(c_code)
    println()
    return 0
}
