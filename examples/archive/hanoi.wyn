fn hanoi(n: int, src: int, dst: int, aux: int) -> int {
    if n == 1 {
        return 1
    }
    hanoi(n - 1, src, aux, dst)
    hanoi(n - 1, aux, dst, src)
    return 1
}

fn main() -> int {
    var moves = hanoi(3, 1, 3, 2)
    print(moves)
    
    moves = hanoi(4, 1, 3, 2)
    print(moves)
    
    return 0
}
