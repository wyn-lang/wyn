fn sum_to_n(n: int) -> int {
    var total = 0
    for var i = 1; i <= n; i += 1 {
        total += i
    }
    return total
}

fn main() -> int {
    var start = time_now()
    
    var result = 0
    for var i = 0; i < 1000; i += 1 {
        result = sum_to_n(100)
    }
    
    var end = time_now()
    var elapsed = end - start
    
    print(result)
    print(elapsed)
    
    return 0
}
