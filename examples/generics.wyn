// Generics Example - Demonstrates generic functions

fn identity<T>(x: T) -> T {
    return x;
}

fn max<T>(a: T, b: T) -> T {
    if a > b {
        return a;
    }
    return b;
}

fn main() -> int {
    let x = identity(42);
    let y = max(10, 20);
    let z = max(5, 3);
    return x + y + z;
}
