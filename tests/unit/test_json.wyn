// Test JSON parser implementation
fn main() -> int {
    // Test simple JSON parsing
    let json_text = "{\"name\":\"John\",\"age\":30}";
    let json = json_parse(json_text);
    
    if json == 0 {
        return 1;
    }
    
    // Test get string
    let name = json_get_string(json, "name");
    if name == 0 {
        return 2;
    }
    
    // Test get int
    let age = json_get_int(json, "age");
    if age != 30 {
        return 3;
    }
    
    json_free(json);
    return 0;
}
