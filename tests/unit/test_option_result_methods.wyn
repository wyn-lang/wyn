// Test Option and Result methods (Phase 5)
fn main() -> int {
    var exit_code = 0;
    
    // Test Option.is_some() and is_none()
    var some_val = some(42);
    if (!(some_val.is_some())) {
        exit_code = exit_code + 1;
    }
    if (some_val.is_none()) {
        exit_code = exit_code + 2;
    }
    
    var none_val = none();
    if (none_val.is_some()) {
        exit_code = exit_code + 4;
    }
    if (!(none_val.is_none())) {
        exit_code = exit_code + 8;
    }
    
    // Test Result.is_ok() and is_err()
    var ok_val = ok(100);
    if (!(ok_val.is_ok())) {
        exit_code = exit_code + 16;
    }
    if (ok_val.is_err()) {
        exit_code = exit_code + 32;
    }
    
    var err_val = err(404);
    if (err_val.is_ok()) {
        exit_code = exit_code + 64;
    }
    if (!(err_val.is_err())) {
        exit_code = exit_code + 128;
    }
    
    return exit_code;
}
