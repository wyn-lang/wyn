// Test that spawn actually creates concurrent threads
// This test spawns multiple workers and verifies main continues immediately

fn worker1() -> int {
    return 1;
}

fn worker2() -> int {
    return 2;
}

fn worker3() -> int {
    return 3;
}

fn main() -> int {
    // Spawn multiple threads
    spawn worker1();
    spawn worker2();
    spawn worker3();
    
    // Main thread returns immediately without waiting
    return 0;
}
