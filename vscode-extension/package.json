{
  "name": "wyn",
  "displayName": "Wyn Language Support",
  "description": "Language support for Wyn programming language",
  "version": "1.5.0",
  "publisher": "wyn-lang",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:wyn"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "wyn",
        "aliases": [
          "Wyn",
          "wyn"
        ],
        "extensions": [
          ".wyn"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "wyn",
        "scopeName": "source.wyn",
        "path": "./syntaxes/wyn.tmLanguage.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^16.0.0",
    "@types/vscode": "^1.60.0",
    "typescript": "^4.5.0"
  },
  "dependencies": {
    "vscode-languageclient": "^7.0.0"
  }
}
